{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "production",
      "prebuildCommand": "npx expo prebuild --platform android"
    },
    "production": {
      "android": {
        "buildType": "app-bundle"
      },
      "channel": "production",
      "prebuildCommand": "npx expo prebuild --platform android"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./path/to/api-key.json",
        "track": "internal"
      }
    }
  }
}

